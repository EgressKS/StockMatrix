    "android": {
      "package": "com.stockmatrix.stockapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "0e5a50f4-bd5d-4c3a-a4ea-41bd7be9d85e"
      }
    },
    "owner": "rootcircles-organization"
  }
