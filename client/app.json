{
  "expo": {
    "name": "stockmatrix-client",
    "slug": "stockmatrix-app",
    "scheme": "stockmatrix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stockmatrix.stockapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.stockmatrix.stockapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"],
      "versionCode": 1
    },

    "extra": {
      "apiBaseUrl": "http://172.70.99.19:3000/api",

      "googleWebClientId": "YOUR_WEB_CLIENT_ID.apps.googleusercontent.com",
      "googleAndroidClientId": "YOUR_ANDROID_CLIENT_ID.apps.googleusercontent.com",
      "googleIosClientId": "YOUR_IOS_CLIENT_ID.apps.googleusercontent.com",

      "eas": {
        "projectId": "7a90a2c9-a3ef-4e3e-a3c4-625375e3d057"
      }
    },

    "plugins": []
  }
}
